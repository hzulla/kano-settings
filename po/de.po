#: ../bin/kano-wifi:467
msgid "Yes,Y/No,N"
msgstr "Ja,J/Nein,N"

#: ../bin/kano-wifi:285
msgid ""
"{{8 x }} {{7 You need root privileges to start this app. Please try sudo }}"
msgstr ""
"{{8 x }} {{7 Dieses Programm benötigt root-Rechte. Bitte starte es mit "
"sudo }}"

#: ../bin/kano-wifi:296
msgid "{{6 - }} {{5 wireless network is not connected }}"
msgstr "{{6 - }} {{5 Keine WLAN-Verbindung }}"

