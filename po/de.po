msgid "{{1 r }} -> Rescan"
msgstr "{{1 s }} -> noch einmal scannen"

#: ../bin/kano-wifi:386
msgid "{{1 n }} -> Next page"
msgstr "{{1 n }} -> nächste Seite"

#: ../bin/kano-wifi:388
msgid "{{1 p }} -> Previous page"
msgstr "{{1 v }} -> vorherige Seite"

#: ../bin/kano-wifi:389
msgid "{{1 h }} -> Connect to hidden network"
msgstr "{{1 u }} -> Verbindung zu unsichtbarem WLAN mit versteckter SSID"

#: ../bin/kano-wifi:390
msgid "{{1 s }} -> Skip"
msgstr "{{1 a }} -> Abbrechen"

#: ../bin/kano-wifi:392
msgid "Choose a network number or option letter:"
msgstr "Wähle ein Netzwerk aus oder wähle eine Option:"

#: ../bin/kano-wifi:393
msgid "Rescan,R/Next,N/Previous,P/Hidden,H/Skip,S"
msgstr "Scan,S/Nächste,N/Vorherige,V/Unsichtbar,U/Abbruch,A"

#: ../bin/kano-wifi:405
msgid "Scanning..."
msgstr "Scanne..."

#: ../bin/kano-wifi:441
#, python-format
msgid "Let's connect to {{2 %s }}"
msgstr "Verbindung zu {{2 %s }}"

#: ../bin/kano-wifi:454
msgid "{{6 - }} {{5 This option is not in range! Please try again }}"
msgstr "{{6 - }} {{5 Unbekannte Option, bitte versuch's noch einmal }}"

#: ../bin/kano-wifi:460
msgid "{{4 + }} {{3 Great! You are connected }}"
msgstr "{{4 + }} {{3 Klasse! Du bist verbunden }}"

#: ../bin/kano-wifi:466
msgid "Do you want to test your connection? {{1 [y]es/[N]o }}"
msgstr "Willst Du die Verbindung jetzt testen? {{1 [j]a/[N]ein }}"

#: ../bin/kano-wifi:469
msgid "Type {{2 ping www.google.com }}"
msgstr "Gib {{2 ping www.google.com }} ein"

#: ../bin/kano-wifi:476
msgid ""
"{{6 - }} {{5 Not the correct command, try again: }} {{2 ping www.google."
"com }}"
msgstr ""
"{{6 - }} {{5 Falsches Kommando, versuch's noch einmal: }} {{2 ping www."
"google.com }}"

#: ../bin/kano-wifi:479
msgid ""
"{{8 x }} {{7 Oops! It seems the Internet is not reachable... Don't worry, we "
"can still play! }}"
msgstr ""
"{{8 x }} {{7 Ups! Das Internet scheint gerade nicht erreichbar zu sein... "
"Keine Sorge, wir können trotzdem spielen! }}"

#: ../bin/kano-wifi:481
msgid "{{4 + }} {{3 Brilliant! Internet is working }}"
msgstr "{{4 + }} {{3 Großartig! Das Internet ist erreichbar }}"

#: ../bin/kano-wifi:484
msgid "Launching the browser so you can log in"
msgstr "Starte jetzt den Browser, damit Du Dich einloggen kannst"

#: ../bin/kano-wifi:487
msgid "Press {{1 ENTER }} to continue"
msgstr "Drücke {{1 ENTER }} und es geht weiter"

