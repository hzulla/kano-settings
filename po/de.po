#: ../bin/kano-wifi:355
#, python-format
msgid "You are connected to {{2 %s }}"
msgstr "Du bist verbunden mit WLAN {{2 %s }}"

#: ../bin/kano-wifi:357
msgid ""
"Do you want to forget this network and choose a new one? {{1 [y]es/[N]o }}"
msgstr ""
"Willst Du dieses WLAN löschen und ein anderes auswählen? {{1 [j]a/[N]ein }}"

#: ../bin/kano-wifi:368
msgid "Help me find the signal."
msgstr "Hilf mir. Ich suche das Signal."

#: ../bin/kano-wifi:377
msgid ""
"{{6 - }} {{5 I can't find any wireless signals. Do you want to scan "
"again? }} {{1 [Y]es/[n]o }}"
msgstr ""
"{{6 - }} {{5 Ich kann kein WLAN entdecken. Willst Du noch einmal scannen? }} "
"{{1 [J]a/[n]ein }}"

