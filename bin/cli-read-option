#!/usr/bin/python

import sys

try:
    description = sys.argv[1].strip()
except IndexError:
    print >> sys.stderr, (
        "Please provide a list of possible input options "
        "and accelerator keys:\n"
        "{} \"Yes,Y/No,N/Exit,X/Reboot,B,R\""
    ).format(sys.argv[0])
    exit()

value = {}
i = 0
for item in description.split('/'):
    i += 1
    for option in item.split(','):
        value[option.strip().upper()] = i

response = raw_input().strip().upper()

print value.get(response, 0)
